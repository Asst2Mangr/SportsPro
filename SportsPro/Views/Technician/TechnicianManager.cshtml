@model List<SportsPro.Models.Technician>

<center><h2>Technician Manager</h2></center>

<table class="table">
    <thread>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Actions</th>
        </tr>
    </thread>
    <tbody>
        @foreach (var technician in Model) //iterates through list and displays info in rows.Also have edit/delete urls made up. 
        {
            <tr>
                <td>@technician.Name</td>
                <td>@technician.Email</td>
                <td>@technician.Phone</td>
                <td>
                      <a href="@Url.Action("Edit", "Technician", new { id = technician.TechnicianID })">Edit</a> 
                      <a href="@Url.Action("Delete", "Technician", new { id = technician.TechnicianID })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<p>
    <a asp-action="Add">Add Technician</a>
</p>