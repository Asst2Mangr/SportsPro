@model Technician

@{
    ViewBag.Title = "Technician Manager";
}

<h2>Delete Technician</h2>


<p> Are you sure you want to delete this technician?</p>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="TechnicianID" />
    <input type ="submit" value="Delete" class="btn btn-primary" />
    <a asp-controller="Technician" asp-action="List" class="btn btn-primary">Cancel</a>
</form>