@model Technician

<h2>Delete Technician</h2>


<p> Are you sure you want to delete this technician?</p>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="TechnicianID" />
    <p>
        <input type ="submit" value="Yes" class="btn btn-danger" />
        <a asp-action="TechnicianManager">No</a>
    </p>

</form>

<a asp-action="TechnicianManager">Back to Technician Manager</a>